<?xml version="1.0" encoding="UTF-8"?>
<quiz>
    <html>
        <q1 id="html">
            <question id="html">"HTML document must always be saved with which of the following?"</question>
            <options id="html">
                ".html"
                ".doc"
                ".doc"
                "both. html and .htm"
            </options>
            <answer id="html">"both .html and .htm"</answer>
        </q1>
    </html>
    <css>
        <q1 id="css">
            <question id="cssq1">"Which of the following CSS property is used to set the backgroun image of an element?"</question>            
            <options id="cssq1">
                <![CDATA["background-color"
                "background-image"
                "background-attachment"
                "none" ]]>
            </options>
            <answer id="cssq1">"background-image"</answer>
        </q1>
        <q2 id="css">
            <question id="cssq2">"How to select the elements with the class name example?"</question>
            <options id="cssq2">
                <![CDATA[ "example"
                "#example"
                "class example"
                ".example"]]>
            </options>
            <answer id="cssq2">".example"</answer>
        </q2>
    </css>
</quiz>